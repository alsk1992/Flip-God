{
  "name": "flipagent",
  "version": "0.1.0",
  "description": "AI-powered e-commerce arbitrage agent. Scans Amazon, eBay, Walmart, AliExpress for price arbitrage, auto-creates listings, fulfills orders via dropship.",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && node -e \"const{cpSync,readdirSync}=require('fs'),p=require('path');readdirSync('src/skills/bundled',{withFileTypes:true}).filter(d=>d.isDirectory()).forEach(d=>{const s=p.join('src/skills/bundled',d.name,'SKILL.md'),t=p.join('dist/skills/bundled',d.name,'SKILL.md');try{cpSync(s,t)}catch{}})\"",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "arbitrage",
    "ecommerce",
    "dropshipping",
    "amazon",
    "ebay",
    "walmart",
    "aliexpress",
    "ai",
    "claude",
    "price-comparison",
    "listing-automation"
  ],
  "author": "alsk1992",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.73.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "pino": "^8.18.0",
    "pino-pretty": "^10.3.1",
    "sql.js": "^1.10.3",
    "ws": "^8.16.0",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.32",
    "@types/sql.js": "^1.4.9",
    "@types/ws": "^8.5.10",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
